{
  "name": "node-devise",
  "version": "0.1.4",
  "description": "Another flexible authentication solution for mongoose.",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src --out-dir dist --ignore *.spec.js --no-comments",
