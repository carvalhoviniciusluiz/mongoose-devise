    "node": ">=8.0.0",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carvalhoviniciusluiz/node-devise.git"
  },
  "author": "Carvalho, Vinicius Luiz <carvalho.viniciusluiz@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carvalhoviniciusluiz/node-devise/issues"
  },
  "homepage": "https://github.com/carvalhoviniciusluiz/node-devise#readme",
  "dependencies": {
    "assert-plus": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mongoose-unique-validator": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "co-mocha": "^1.2.2",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.1.4",
    "pre-commit": "^1.2.2"
